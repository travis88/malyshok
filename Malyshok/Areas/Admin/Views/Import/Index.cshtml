@model ImportViewModel
@if (Model.UserResolution.Read)
{
    <div class="content">
        @Html.TextBox("upload", null, new { type = "file", title = "xml-документ", accept = ".xml", @class = "noplagin", multiple = "multiple" })
        <br />
        <br />
        <div id="content"></div>
        <script src="~/Content/dist/bundle.js"></script>
        <link href="~/Content/css/import-react.css" rel="stylesheet" />
    </div>
    <div class="dop_info-block"></div>
    <div class="buttons">
        <button type="submit" name="action" data-action="noPreloader-accept" value="xml-btn" class="button icon-sitemap">Импортировать</button>
    </div>
}