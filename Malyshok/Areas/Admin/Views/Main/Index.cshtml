                    <div class="log-item">
                        <div class="date-col"> 
                            @ItemLog.Date
                        </div>
                        <div class="action-col">
                            @ItemLog.Action
                        </div>
                        <div class="action-col">
                            @ItemLog.PageName
                        </div>
                        <div class="user-col">
                            @if (ItemLog.Site!=String.Empty) { @ItemLog.Site<br /> }
                            <a target="_blank" title="" href="/admin/@ItemLog.Section/item/@ItemLog.PageId">@ItemLog.PageName</a>
                        </div>
                    </div>
                }
            </div>
