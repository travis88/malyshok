@{
    Layout = null;
}

@if (Model != null)
{
    <div>
        <nav aria-label="Page navigation">
            <ul class="pagination">
                @foreach (PagerFront item in Model)
                {
                    string active = (@item.isChecked) ? "PagerNow active" : "";
                    <li class="@active"><a href="@item.url">@item.text</a></li>
                }
            </ul>
        </nav>
    </div>
}
