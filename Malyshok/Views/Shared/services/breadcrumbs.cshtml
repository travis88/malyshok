@{
    Layout = null;
}
@model Breadcrumbs[]

<div>
@if (Model != null)
{
    <ol class="breadcrumb noprint">
        <li class="breadcrumb-item"><a href="/">Главная</a></li>
        @foreach (var item in Model)
        {
            <li class="breadcrumb-item"><a href="@item.Url">@item.Title</a></li>
        }
    </ol>
}
</div>


